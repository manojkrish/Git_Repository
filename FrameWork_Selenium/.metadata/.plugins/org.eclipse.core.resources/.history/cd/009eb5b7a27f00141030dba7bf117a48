package training_Java;

public class Function_Basic {
	
	public static void main(String args[])
	{
		int s=hit(3,4,5);
		System.out.println(s);
		hit(4,5,6);
		System.out.println("--");
		hit(7,8,9);
		System.out.println("--");
	}
	
	public static int hit(int a,int b, int c)
	{
		int sum = a+b+c;
		
		System.out.println(+sum);
		
		return sum;
		
	}

}
