package accessModifier;

class A{  

	private int data=40;  

	private void msg()
	
	{
		
		System.out.println("Hello java");
		
	}  
	
	A()
	{
		int source = data;
		
		msg();
		
		
	}
}  
  
public class PrivateAccess{  
 
	public static void main(String args[])
	{  
   
		A obj=new A();  
   
		
   
	}  
}  