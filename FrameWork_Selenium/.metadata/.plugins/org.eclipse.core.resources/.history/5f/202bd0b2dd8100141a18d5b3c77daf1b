package java_tut;

class Tutorial_java {
	
	double price;
	int number_of_pages;
	String book_name;
	String author_name;
	double weight_of_the_book;
	


Tutorial_java(double a,int b,String c,String d,double e){
	
	price=a;
	number_of_pages=b;
	book_name=c;
	author_name=d;
	weight_of_the_book=e;
}

Tutorial_java TJ = new Tutorial_java(175,1024,"Java","Mike",175.89);

{

	System.out.println(TJ.price);

}
}