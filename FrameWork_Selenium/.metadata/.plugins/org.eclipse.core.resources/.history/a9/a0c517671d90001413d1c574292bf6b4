package java_tut;

public class Init  {

	public static void main(String[] args) {
		
		
		Browser br = new Browser();
		
		br.URL = "http://www.google.com";
		
		br.username = "Manoj";
		
		br.password = "Manoj";
		
		br.displayUrl();
		
		br.status ="Active";
		
		br.pageName ="Google_Main_Page";
		
		br.forgetPassword = "http://www.google.com/forget/link";
		
		br.pageInfo(br.status, br.pageName, br.forgetPassword);
		
		br.stat(br.next, br.previous, br.sts);
		

	}

}
