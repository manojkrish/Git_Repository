package java_tut;

public class Login {
	
	String username;
	
	String password;
	
	void LoginAcess()
	{
		
		System.out.println(username);
		
		System.out.println(password);
		
		if(username == " Manoj" && password =="Manoj")
		{
			System.out.println("Login Successfull");
		}
	
		System.out.println("Login Succcessful");
	}

}
