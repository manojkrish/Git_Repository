package java_tut;

class This{
	
	int Age;
	
	String Name;
	
	void display()
	{
		System.out.println(Age);
		
		System.out.println(\n);
		
		System.out.println(Name);
	}
	
	public static void main(String[] args)
	{
		
		This t = new This();
		
		t.Age =29;
		
		t.Name="Manoj";
		
		t.display();
		
	}
}