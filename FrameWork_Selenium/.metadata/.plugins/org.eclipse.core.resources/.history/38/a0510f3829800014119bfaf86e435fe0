package training_Java;

public class Function_Basic {
	
	public static void main(String args[])
	{
		int s=hit(3,4,5);
		
		System.out.println(s);
		
		int t=git('h',4,5);
		
		System.out.println(t);
		
	}
	
	public static int hit(int a,int b, int c)
	{
		int sum = a+b+c;
		
		return sum;
		
	}
	
	public static int git(char a,int b,int c)
	{
		char d =a;
		
		int sum = b +c;
		
		return sum;
	}
}
