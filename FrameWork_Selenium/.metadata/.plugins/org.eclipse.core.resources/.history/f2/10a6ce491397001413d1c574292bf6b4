package thread;

public class ThreadWithinParam {

	public static void main(String[] args) {
		
		Thread t1 = new Thread(new Runnable(){

			@Override
			public void run() {
				// TODO Auto-generated method stub
				for(int i=0;i<=10;i++){
					System.out.println("Thread count is : "+i);
				}
				
			}
			
		})
	

	}

}
