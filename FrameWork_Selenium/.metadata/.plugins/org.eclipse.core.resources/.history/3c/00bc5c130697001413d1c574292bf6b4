package thread;

public class FirstThread extends Thread
{
    public void run()
    {  
    System.out.println("thread is running...");  
    }  
    public static void main(String args[])
    {  
    
    	FirstThread t1=new FirstThread();  
    	
    	FirstThread t2 = new FirstThread();
    
    	t1.start();  
    	
    	t2.start();
     
    }  
   }  
