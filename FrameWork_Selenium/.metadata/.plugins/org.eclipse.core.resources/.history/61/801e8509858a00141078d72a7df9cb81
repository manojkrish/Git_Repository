package java_tut;

public class Tutorial {
	
	public static void main(String[] args)
	{
		System.out.println("Hello Boss");
		
		double x = 45;
		double y = 10.5;
		
		double sum = ( x+y);
		
		System.out.println("the value of the sum is : " + sum);
		
		String Name = "login";
	
		System.out.println(Name);
		
		if(Name =="logi")
		{
					
			System.out.println("Login is pass");
		
		}
		else
		{
		
			System.out.println("Login is fail");
		
		}
		
		String Username ="Manoj";
		
		String Password ="ABCDE";
		
		if (Username == "Manoj" && Password == "ABCD")
		{
			
			System.out.println("Login Successfull and entered into application");	
		
		}
		else 
		{
		
			System.out.println("Login is not Successful and failed to entered into application");
		
		}	
		
		//String Admin = "Manoj";
		//String Admin = "Phil";
		
		String Admin ="George";
		
		if (Admin == "Manoj")
		{
		
			System.out.println("Manoj is the Administrator");
		
		}
		else if(Admin =="Phil")
		{
		
			System.out.println("Phil is the User");
		
		}
		else if(Admin =="George")
		{
		
			System.out.println("George is the member");
		
		}
		for(int i=1;i<=5;i++)
		{
		
			String  sum1 = "http://www.google.com";
			
			System.out.println(sum1);
			
		}
		
		int  z=-10;
		while ( z<=0)
		{
			
			System.out.println("The values of the z values are shown here as " +z*-1);
			
			z++;
			
		}
		
		int  d=-10;
		do
		{
			
			System.out.println("Enter into the loop" +d);
			
			d++;
			
			System.out.println("Value after the decrement of d:"+d);
		
		}
		while(d>0);
		
		System.out.println("Escape from the do while loop");
		
		System.out.println("Values of the Do while loop printed");
		
		int a,b;
		
		a=10;
		b=-10;
		
		if (a > 0 && b > 0)
		{
			System.out.println("A and B are the positive numbers "+a+b);
		}
		else if(a>0 && b>0)
		{
			System.out.println("One of the number is negative zero");
		}
		else if (a>0 && b<0)
		{
			System.out.println("A is the +ve number and B is the -ve number");
		}
		else if(a<0 && b>0)
		{
			System.out.println("A is the -ve number and B is the +ve number");
		}
		
		
	}
}