package training_Java;

public class For_Loop {

	public static void main(String[] args) 
	{
		
		for(int i=0;i<=10;i++)
		{
			for(int j =0;j<=10;j++)
			{	
				for(int k=0:k<=10;k++)
				{
					System.out.println(k);
				}
			}
		}
		
		int sum=0;
		for(int i=0;i<=100;i++)
		{
			sum=sum+i;
		}
		
		System.out.println(+sum);
	}

}
