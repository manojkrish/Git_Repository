package Excel_Usage;
import java.io.FileInputStream;

public class Get_Row_CountIn_Excel {
	
	static int RowCount = 0;
	// initializing row count as Zero
	
	public static void main(String[] args)
	{
		FileInputStream fis = new FileInputStream(F:\Git_Repository\FrameWork_Selenium\DataDriven_Project\src\testdata.xlsx);
		
		Workbook wb= new Workbook.create(fis);
		
		Sheet s = wb.("Sheet1");
		
		RowCount = s.getLastRowNum();
		
		System.out.println(RowCount);
		
		
	}

}
