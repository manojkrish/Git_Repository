package com.obj.TestScript;

import org.openqa.selenium.WebDriver;
import org.testng.annotations.Test;
//import com.obj.BrowserOpen.OpenBrowser;
import com.obj.BrowserOpen.TweakBrowser;
import com.obj.Login.LoginPage;
//import com.thoughtworks.selenium.Wait;

public class TestScript extends TweakBrowser

{
	
	@Test(priority = 1)
	public void loginclick() throws InterruptedException
	{
		
		LoginPage LP = new LoginPage(driver);
		
		LP.Firstlogin();
		
		Thread.sleep(6000);
		
	}
	
	@Test
	public void LoginInput() throws InterruptedException
	{
		LoginPage LP = new LoginPage(driver);
		
		LP.logininput();
		
		Thread.sleep(5000);
		
	}
	
	@Test
	public void PasswordInput() throws InterruptedException
	{
		LoginPage LP = new LoginPage(driver);
		
		LP.Passwordinput();
		 
		Thread.sleep(6000);
	}
	
	@Test
	public void LoginClick() throws InterruptedException
	{
		LoginPage LP = new LoginPage(driver);
		
		LP.clickloginbutton();
		
		Thread.sleep(2000);
		
	}

}
