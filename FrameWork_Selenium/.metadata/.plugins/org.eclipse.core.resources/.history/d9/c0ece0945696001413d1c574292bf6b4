package java_tut;

public class Sample2 {
	
	double SimpleCalc(double p,double r,double t)
	{
		
		double SI;
		
		 SI =p*r*t/100;
		 
		 return SI;
		
	
	}
	
	public static void main(String[] args)
	{
		
		Sample2 s2 = new Sample2();
		
		s2.SimpleCalc(1000,10.5, 4);
		
		
		
		
		
			
		
		
		
	}

}
