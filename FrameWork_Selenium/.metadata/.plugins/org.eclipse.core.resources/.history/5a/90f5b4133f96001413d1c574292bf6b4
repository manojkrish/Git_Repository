package array;

public class SumArry {

	public static void main(String[] args) {
		
		int a[] ={12,24,28,30,32};
		
		String b[]={"Jim","Kim","Duke","Mike","Ben"};
		
		
		for (int j=0 ;j<b.length;j++)
		{
		
		for(int i=0;i<j;i++)
		{
			
			System.out.println("Canditate name and the age are calculated and displayed here : " +a[i]);
		
		}
		}
	}
}