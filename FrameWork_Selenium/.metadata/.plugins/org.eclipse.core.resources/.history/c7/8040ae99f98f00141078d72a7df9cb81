package java_tut;

public class ObjClas {
	
	private int id;
	
	private String name;
	
	ObjClas(int id,String name)
	{
		this.id = id;
		
		this.name = name;
	}
	
		public String toString()
		{
			 return id +"     "+name;
		}
	
	public static void main(String[] args)
	{
		ObjClas OC = new ObjClas(12,"Phil");
		
		System.out.println(OC);
	}

}
