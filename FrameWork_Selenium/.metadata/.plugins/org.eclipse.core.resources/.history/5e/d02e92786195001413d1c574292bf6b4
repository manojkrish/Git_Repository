package Except;

public class excep {
	
	public static void main(String[] args)
	{
		
		try
		
		{
			
			int a = 8;
			
			int b =0;
			
			int c=a/b;
			
			System.out.println(c);
			
		}
		
		catch(Exception ex)
		
		{
			
			System.out.println(ex);
		
		}
		
		try{
			
			int a[] = new int[6];
			
			System.out.println(a[7]);
			
		}
		
		catch(Exception e){
			
			System.out.println(e);
		}
	}

}
