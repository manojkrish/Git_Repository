package training_Java;

public class car {

	static String Model;
	int price;
	int FuelCapacity;
	
	public static void main(String[] args) {
		
		
		car c1 = new car();
		
		Model="Mercedes-Benz";
		
		System.out.println(Model);
		
		c1.start();
		
		c1.Stop();
		
		car c2 = new car();
		
		Model="Ferrari";
		
		System.out.println(Model);
		 
		c2.start();
		
		c2.Stop();
		
		
		
	}
		
		public void start()
		{
			System.out.println("Start");
		}
		
		public void Stop()
		{
			System.out.println("Stop");
		}
		
		
		
		
		

	}
