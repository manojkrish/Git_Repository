package session1;

public class Day1 {
	
	
	String name;
	
	String color;
	
	int age;
	
	void display()
	{
		
		System.out.println(name);
		
		System.out.println(color);
		
		System.out.println(age);
	}
	
	public static void main(String[] args)
	{
		 
		Day1 dy = new Day1();
		
		dy.age =25;
		
		dy.color ="blue";
		
		dy.name="Mike";
		
		dy.display();
		
		System.out.println("Foo");
	}
	
}
