package session1;

public class Person {
	
	int age;
	
	static String name;
	
	static String companyName;
	
	public static void main(String[] args)
	{
		Person p = new Person();
		
		p.age=12;
		
		Person.name="Phil";
		
		Person.companyName ="Pearson";
		
		System.out.println(Person.companyName+"     "+p.age+"    "+Person.name);
		
	}

}
