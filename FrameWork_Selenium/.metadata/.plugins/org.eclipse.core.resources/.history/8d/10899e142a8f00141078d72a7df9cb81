package java_tut;

class This{
	
	private int Age;
	
	private String Name;
	
	void  getAge(int Age)
	{
		this. Age = Age;
		 
		 System.out.println(Age);
	}
	
	void getName(String Name)
	{
		this.Name = Name;
		
		System.out.println(Name);
	}
	
	public static void main(String[] args)
	{
		
		This t = new This();
		
		t.getAge(30);
		
		t.getName("Mike");
				
	}
}