		System.out.println("the value of the sum is : " + sum);
		
		String Name = "login";
	
		System.out.println(Name);
		
		if(Name =="logi")
		{
					
			System.out.println("Login is pass");
		
		}
		else
		{
		
			System.out.println("Login is fail");
		
		}
		
		String Username ="Manoj";
		
		String Password ="ABCDE";
		
