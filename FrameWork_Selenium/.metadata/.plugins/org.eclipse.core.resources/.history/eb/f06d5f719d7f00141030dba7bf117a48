package training_Java;

public class Two_Dimensional_Array 
{

	public static void main(String[] args) 
	{
		
								/// Rows & columns
		String Str[][]=new String [3][4];
		
		Str [0][0]="A";
		Str [0][1]="B";
		Str [0][2]="C";
		Str [0][3]="D";
		
		Str [1][0]="A1";
		Str [1][1]="B1";
		Str [1][2]="C1";
		Str [1][3]="D1";
		
		Str [2][0]="A1";
		Str [2][1]="B1";
		Str [2][2]="C1";
		Str [2][3]="D1";
		
		System.out.println("Rows are"+Str.length);
		
		System.out.println("Columns are" +Str[0][0].length);
		
		
		
		
		
	}

}
